    "formula_fortuna_tot": "int(int(attore_attivo['fortuna_base']) +int(attore_attivo['fortuna_bonus_extra']) + int(attore_attivo['livello']) / 5 \u2013 0.15)",
    "formula_forza_tot": "int(int(attore_attivo['forza_base']) + int(attore_attivo['forza_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_agilita_tot": "int(int(attore_attivo['agilita_base']) + int(attore_attivo['agilita_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_velocita_tot": "int(int(attore_attivo['velocita_base']) + int(attore_attivo['velocita_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_intelligenza_tot": "int(int(attore_attivo['intelligenza_base']) + int(attore_attivo['intelligenza_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_concentrazione_tot": "int(int(attore_attivo['concentrazione_base']) + int(attore_attivo['concentrazione_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_personalita_tot": "int(int(attore_attivo['personalita_base']) + int(attore_attivo['personalita_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_saggezza_tot": "int(int(attore_attivo['saggezza_base']) + int(attore_attivo['saggezza_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_resistenza_tot": "int(int(attore_attivo['resistenza_base']) + int(attore_attivo['resistenza_bonus_extra']) + int(attore_attivo['livello']) / 5 + int(int(attore_attivo['fortuna_tot']) - 10) * 0.15 \u2013 0.15)",
    "formula_pf_base": "15 + (int(attore_attivo['forza_tot'])-10)*3  + (int(attore_attivo['resistenza_tot'])-10) * 3",
    "formula_pf_tot": "int((int(attore_attivo['pf_base']) + int(attore_attivo['pf_item']) + int(attore_attivo['pf_extra'])  +int(attore_attivo['pf_temp']))  * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12)) / 100)",
    "formula_mana_base": "10 + (int(attore_attivo['intelligenza_tot'])-10) * 3 + (int(attore_attivo['concentrazione_tot']) -10)* 3",
    "formula_mana_tot": "int((int(attore_attivo['mana_base']) + int(attore_attivo['mana_item']) + int(attore_attivo['mana_extra'])) * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12))  / 100)",
    "formula_energia_base": "6 + (int(attore_attivo['resistenza_tot'])-10) + (int(attore_attivo['velocita_tot'])-10)",
    "formula_energia_tot": "int((int(attore_attivo['energia_item']) + int(attore_attivo['energia_extra']) + int(attore_attivo['energia_base']))  * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12))/ 100)",
    "formula_potere_base": "2+ (int(attore_attivo['intelligenza_tot'])-10) + (int(attore_attivo['saggezza_tot'])-10)",
    "formula_potere_tot": "int((int(attore_attivo['potere_base']) + int(attore_attivo['potere_item']) + int(attore_attivo['potere_extra'])) * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12))  / 100)",
    "formula_attacco_base": "5 + (int(attore_attivo['forza_tot']) -10 ) + (int(attore_attivo['agilita_tot']) -10)",
    "formula_attacco_tot": "int((int(attore_attivo['attacco_arma_attuale']) + int(attore_attivo['attacco_item']) + int(attore_attivo['attacco_extra']) + int(attore_attivo['attacco_base']) + int(attore_attivo['attacco_temp'])) * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12)) / 100)",
    "formula_difesa_base": "15+ (int(attore_attivo['agilita_tot'])-10)  + (int(attore_attivo['concentrazione_tot'])-10)",
    "formula_difesa_tot": "int((int(attore_attivo['difesa_base']) + int(attore_attivo['difesa_temp']) + int(attore_attivo['difesa_extra']) + int(int(attore_attivo['difesa_armatura'])) + int(attore_attivo['difesa_item']))  * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12))/ 100)",
    "formula_pa_base": "8 + (int(attore_attivo['velocita_tot'])-10) *0.75 + (int(attore_attivo['saggezza_tot'])-10) * 0.75",
    "formula_pa_tot": "int((int(attore_attivo['pa_item']) + int(attore_attivo['pa_extra']) + int(attore_attivo['pa_base']) - (int(attore_attivo['carico']) / 4)) * (100 \u2013 (int(attore_attivo['stanchezza']) * 8) + (int(attore_attivo['modificatore_generale']) * 12)) / 100)",
    "formula_barr_fis_tot": "int(attore_attivo['barr_fis_item']) + int(attore_attivo['barr_fis_extra'])",
    "formula_barr_mag_tot": "int(attore_attivo['barr_mag_item']) + int(attore_attivo['barr_mag_extra'])",
    "formula_compri_a_meno_tot": "-int(attore_attivo['compri_a_meno_extra']) - int(attore_attivo['personalita_tot'])",
    "formula_vendi_a_piu_tot": "20 + int(attore_attivo['vendi_a_piu_extra']) + int(attore_attivo['personalita_tot']) / 2",
    "formule_speciali": "attore_attivo['energia_item'] += 0$$if attore_attivo['eq_scudo'][\u201cTIPO2\u201d] in Oggetti.materiali_leggeri :    attore_attivo['attacco_item'] += 0$$if attore_attivo['note_zaino_1'] == '500':    attore_attivo['danno'] += 0",
